<?import javafx.scene.image.ImageView?>

<StackPane stylesheets="@style.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox alignment="CENTER" prefHeight="449.0" prefWidth="502.0" spacing="20" styleClass="main-root">
         <children>
            <ImageView fx:id="logoImage" fitHeight="275.0" fitWidth="180.0">
               <image>
                  <Image url="@images/foxLogo.png" />
               </image>
            </ImageView>
            <Label styleClass="main-title" stylesheets="@style.css" text="SwiftFox" />
            <Label styleClass="glowing-text" stylesheets="@style.css" text="Visualize and simulate CPU scheduling algorithms in real-time!" />
            <Button fx:id="launchBtn" text="Launch Simulator" />
         </children>
      </VBox>
   </children>
</StackPane>
